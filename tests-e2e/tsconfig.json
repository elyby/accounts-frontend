{
  "compilerOptions": {
    "strict": true,
    "baseUrl": "../node_modules",
    "module": "esnext",
    "target": "es5",
    "esModuleInterop": true,
    "noEmit": true,
    "moduleResolution": "node",
    "lib": ["es6", "dom"],
    "types": ["cypress", "@types/testing-library__cypress"],
    "resolveJsonModule": true,
    "noImplicitAny": false
  },
  "include": ["cypress/**/*"]
}
